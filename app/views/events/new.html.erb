<h1>Events#new</h1>
<%= form_for(@event, html: { multipart: true }) do |f| %>
  <p>
    <%= f.label :event_title %><br/>
    <%= f.text_field :event_title%><br/>
  </p>
  <p>
    <%= f.label :event_content %><br/>
    <%= f.text_area :event_content%><br/>
  </p>
  <p>
    <%= f.label :event_date %><br/>
    <%= f.text_field :event_date%><br/>
  </p>
  <span class="picture">
    <%= f.file_field :picture %>
  </span>
  <p>
    <%= f.submit "Add Event"%>
    
  </p>
<% end %>

